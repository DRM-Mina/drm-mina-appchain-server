{
    "name": "express",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "type": "module",
    "main": "server.ts",
    "watch": {
        "build-and-start": {
            "patterns": [
                "src"
            ],
            "extensions": [
                "*"
            ]
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
